+                        }],
+                    from: id,
+                    messageName: 'GET_PROVIDERS',
+                    messageType: MessageType.GET_PROVIDERS
+                };
+                yield event;
+            }());
+            const result = await all(client.dht.getClosestPeers(cid.bytes));
+            expect(result).to.have.lengthOf(1);
+            expect(result[0].id.equals(id)).to.be.true();
+        });
