+            expect(encodeHeader(header)).to.deep.equal(encodeHeaderNaive(header));
+            expect(decodeHeader(encodeHeader(header))).to.deep.equal(decodeHeaderNaive(encodeHeaderNaive(header)));
+        });
+    }
