    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "build": "aegir build"
  },
  "dependencies": {
    "@libp2p/interface-compliance-tests": "^3.0.0",
    "@libp2p/interface-peer-discovery": "^2.0.0",
    "@libp2p/interfaces": "^3.0.0",
    "@multiformats/multiaddr": "^12.1.3",
    "aegir": "^39.0.5",
    "delay": "^6.0.0",
    "p-defer": "^4.0.0"
